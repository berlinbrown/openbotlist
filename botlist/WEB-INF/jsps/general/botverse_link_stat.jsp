/** =============================================== */
/** == Link List Data, Only for the Link Section == */
/** == Bold and Dark Blue / Purple               == */
/** == This may also be used for the 'index' style  */
/** =============================================== */
a.linklist_objlinks:link {
	font-family: Tahoma, Arial, Helvetica, sans-serif;
	font-size: 11pt;
	font-weight: bold;
	margin-bottom: 0px;
	margin-top: 0px;
	background-color: transparent;
	/** New entity link foreground color */
	color: #2327ae;
	text-decoration: underline;
}

/** Leave the visited link as darker than others */
a.linklist_objlinks:visited {
	font-family: Tahoma, Arial, Helvetica, sans-serif;
	font-size: 11pt;
	font-weight: bold;
	margin-bottom: 0px;
	margin-top: 0px;
	color: #460C7B;
	background-color: transparent;
	text-decoration: underline;
}

a.linklist_objlinks:hover {
	font-family: Tahoma, Arial, Helvetica, sans-serif;
	font-size: 11pt;
	font-weight: bold;
	margin-bottom: 0px;
	margin-top: 0px;
	color: #447;
	background-color: transparent;
	text-decoration: underline;
}
a.linklist_objlinks:active {
	font-family: Tahoma, Arial, Helvetica, sans-serif;
	font-size: 11pt;
	font-weight: bold;
	margin-bottom: 0px;
	margin-top: 0px;
	color: #447;
	background-color: transparent;
	text-decoration: underline;
}

